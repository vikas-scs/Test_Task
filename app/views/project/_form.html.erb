<h1>Clone Project</h1>
  <%= form_with(model: @projects) do |form| %>
  <div>
    <%= form.label :Project_name %><br>
    <%= form.text_field :name %>
    <% @project.errors.full_messages_for(:name).each do |message| %>
      <div><%= message %></div>
      <% end %>
  </div>
  <div>
    <%= form.label :enter_URL %><br>
    <%= form.text_field :github_url %>
</div>
<div class="actions">
    <%= form.submit "clone" %>
 </div>
 <% end %>